
public class Client {
	private String nom;
	private Serveur serveurActif;
	
	public Client(String nomClient, Serveur serveurclient) {
		this.nom = nomClient;
		this.serveurActif = serveurclient;
	}
	
	public void envoyer(Message message) {
		this.serveurActif.stocker(message);
	}
	
	public void envoyer(String contenu, Client destinataire)
	
	public void consulter() {}
}
